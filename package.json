{
  "name": "qr-scanner-plugin",
  "version": "0.0.2",
  "description": "A plugin to interface with zebra qr scanners",
  "main": "dist/plugin.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "lint": "npm run prettier -- --check && npm run swiftlint -- lint",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "build": "npm run clean && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build",
    "ionic:build": "npm run build"
  },
  "author": "Ayush Suresh",
  "license": "MIT",
  "devDependencies": {
    "@capacitor/android": "5.4.0",
    "@capacitor/core": "5.4.0",
    "@capacitor/ios": "5.4.0",
    "@ionic/prettier-config": "4.0.0",
    "@ionic/swiftlint-config": "^1.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "prettier": "^2.0.5",
    "prettier-plugin-java": "^0.8.0",
    "rimraf": "5.0.1",
    "rollup": "^2.21.0",
    "swiftlint": "^1.0.1",
    "typescript": "5.2.2"
  },
  "peerDependencies": {
    "@capacitor/core": "5.4.0"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "QrScannerPlugin.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/ayushboss/zebra-qrscanner-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/ayushboss/zebra-qrscanner-plugin.git/issues"
  }
}